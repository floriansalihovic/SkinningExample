<flex-config xmlns="http://www.adobe.com/2006/flex-config">
  <compiler>
    <namespaces>
      <namespace>
        <uri>http://www.icodeapps.net</uri>
        <manifest>manifest.xml</manifest>
      </namespace>
    </namespaces>
    <debug>true</debug>
    <source-path>
      <path-element>src</path-element>
      <path-element>theme</path-element>
    </source-path>
    <theme>
      <filename>theme/skins.css</filename>
    </theme>
  </compiler>
  <static-link-runtime-shared-libraries>true</static-link-runtime-shared-libraries>
  <file-specs>
    <path-element>src/SkinningExample.mxml</path-element>
  </file-specs>
  <output>out/production/SkinningExample/SkinningExample.swf</output>
</flex-config>